{
  "globalHeaders": {
    "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline'; img-src 'self' data:; font-src 'self';"
  },
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/",
      "headers": {
        "Cache-Control": "must-revalidate, max-age=604800"
      }
    },
    {
      "route": "/index.html",
      "headers": {
        "Cache-Control": "must-revalidate, max-age=604800"
      }
    },
    {
      "route": "/redmuffin.Blazor.StaticWeb.styles.css",
      "headers": {
        "Cache-Control": "must-revalidate, max-age=604800"
      }
    },
    {
      "route": "/css/*",
      "headers": {
        "Cache-Control": "must-revalidate, max-age=604800"
      }
    },
    {
      "route": "/lib/fontAwesome-free/webfonts/*",
      "headers": {
        "Cache-Control": "must-revalidate, max-age=604800"
      }
    },
    {
      "route": "/sample-data/*",
      "headers": {
        "Cache-Control": "must-revalidate, max-age=604800"
      }
    },
    {
      "route": "/_framework/*",
      "headers": {
        "Cache-Control": "must-revalidate, max-age=604800"
      }
    },
    {
      "route": "/favicon.png",
      "headers": {
        "Cache-Control": "must-revalidate, max-age=604800"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/sample-data/*",
      "/css/*",
      "/lib/fontAwesome-free/webfonts/*"
    ]
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}