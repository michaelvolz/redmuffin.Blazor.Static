{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/",
      "headers": {
        "Cache-Control": "must-revalidate, max-age=604800"
      }
    },
    {
      "route": "/index.html",
      "headers": {
        "Cache-Control": "must-revalidate, max-age=604800"
      }
    },
    {
      "route": "/css/*",
      "headers": {
        "Cache-Control": "must-revalidate, max-age=604800"
      }
    },
    {
      "route": "/_framework/*",
      "headers": {
        "Cache-Control": "must-revalidate, max-age=604800"
      }
    },
    {
      "route": "/favicon.png",
      "headers": {
        "Cache-Control": "must-revalidate, max-age=604800"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/sample-data/*",
      "/css/*"
    ]
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}